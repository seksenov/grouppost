<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-us" Name="groupPost">
    <CommandPrefix>Group Post</CommandPrefix>
    <Example>Group Post add note</Example>
    <Command Name="addNote">
      <Example>add a note hello</Example>
      <ListenFor>[please] [add] [a] note {noteSubject}</ListenFor>
      <Feedback>adding {noteSubject} to Group Post</Feedback>
      <Navigate/>
    </Command>
	<Command Name="showNote">
		<Example>Show Notes</Example>
		<ListenFor>show [my] notes</ListenFor>
		<Feedback>Showing your notes</Feedback>
		<Navigate/>
	</Command>
    <PhraseTopic Label="noteSubject" Scenario="Dictation">
    </PhraseTopic>
  </CommandSet>
</VoiceCommands>